.blogs
  - if @posts.size == 0
    .nocontent
      = t(:nocontent)
  - else
    = render :partial=> "post", :collection=> @posts
  - if @has_old
    .has_old
      a.pagination href="#" data-page="#{@page + 1}" #{t(:prev)}
  - if @has_new
    .has_new
      a.pagination href="#" data-page="#{@page - 1}" #{t(:next)}
